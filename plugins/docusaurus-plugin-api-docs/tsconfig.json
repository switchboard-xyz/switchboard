{
  "extends": "tsconfig-moon/tsconfig.projects.json",
  "compilerOptions": {
    "emitDeclarationOnly": false,
    "noEmit": false,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "composite": true,
    "declarationDir": "lib",
    "outDir": "lib",
    "rootDir": "src",
    "lib": ["esnext", "dom"],
    "jsx": "react",
    "types": [
      "node",
      "@docusaurus/module-type-aliases",
      "@docusaurus/theme-classic"
    ]
  },
  "include": ["src/**/*"],
  "references": []
}
