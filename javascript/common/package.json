{
  "name": "@switchboard-xyz/common",
  "version": "3.2.0",
  "description": "common type definitions and utils for interacting with switchboard",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js",
      "require": "./index.cjs"
    },
    "./esm-utils": {
      "types": "./esm-utils.d.ts",
      "import": "./esm-utils.js",
      "require": "./esm-utils.cjs"
    },
    "./protos": {
      "types": "./protos.d.ts",
      "import": "./protos.js",
      "require": "./protos.cjs"
    },
    "./networks": {
      "types": "./networks.d.ts",
      "import": "./networks.js",
      "require": "./networks.cjs"
    },
    "./secrets": {
      "types": "./secrets.d.ts",
      "import": "./secrets.js",
      "require": "./secrets.cjs"
    },
    "./utils/async": {
      "types": "./utils/async.d.ts",
      "import": "./utils/async.js",
      "require": "./utils/async.cjs"
    },
    "./utils/axios": {
      "types": "./utils/axios.d.ts",
      "import": "./utils/axios.js",
      "require": "./utils/axios.cjs"
    },
    "./utils/big": {
      "types": "./utils/big.d.ts",
      "import": "./utils/big.js",
      "require": "./utils/big.cjs"
    },
    "./utils/non-empty-array": {
      "types": "./utils/non-empty-array.d.ts",
      "import": "./utils/non-empty-array.js",
      "require": "./utils/non-empty-array.cjs"
    },
    "./utils/number": {
      "types": "./utils/number.d.ts",
      "import": "./utils/number.js",
      "require": "./utils/number.cjs"
    },
    "./utils/oracle-job": {
      "types": "./utils/oracle-job.d.ts",
      "import": "./utils/oracle-job.js",
      "require": "./utils/oracle-job.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./index.js",
  "types": "./index.d.ts",
  "files": [
    "lib/",
    "package.json",
    "index.cjs",
    "index.js",
    "index.d.ts",
    "esm-utils.cjs",
    "esm-utils.js",
    "esm-utils.d.ts",
    "protos.cjs",
    "protos.js",
    "protos.d.ts",
    "networks.cjs",
    "networks.js",
    "networks.d.ts",
    "secrets.cjs",
    "secrets.js",
    "secrets.d.ts",
    "utils/async.cjs",
    "utils/async.js",
    "utils/async.d.ts",
    "utils/axios.cjs",
    "utils/axios.js",
    "utils/axios.d.ts",
    "utils/big.cjs",
    "utils/big.js",
    "utils/big.d.ts",
    "utils/non-empty-array.cjs",
    "utils/non-empty-array.js",
    "utils/non-empty-array.d.ts",
    "utils/number.cjs",
    "utils/number.js",
    "utils/number.d.ts",
    "utils/oracle-job.cjs",
    "utils/oracle-job.js",
    "utils/oracle-job.d.ts"
  ],
  "scripts": {
    "prebuild": "pnpm lint",
    "build": "pnpm exec rimraf lib lib-cjs && node ./compile-protos.js && echo '!src/esm-utils.js' >> .gitignore",
    "clean": "pnpm exec rimraf node_modules lib .turbo",
    "lint": "eslint \"src/**/*.ts\" --ignore-pattern \"src/**/*.d.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --ignore-pattern \"src/**/*.d.ts\" --fix",
    "prepack": "pnpm build",
    "test": "pnpm exec jest",
    "test:bun": "bun exec jest"
  },
  "dependencies": {
    "@solana/web3.js": "^1.98.2",
    "axios": "^1.9.0",
    "big.js": "^6.2.2",
    "bn.js": "^5.2.1",
    "bs58": "^6.0.0",
    "buffer": "^6.0.3",
    "decimal.js": "^10.4.3",
    "js-sha256": "^0.11.0",
    "protobufjs": "^7.4.0",
    "yaml": "^2.6.1"
  },
  "devDependencies": {
    "@types/big.js": "^6.1.6",
    "@types/bn.js": "^5.1.5",
    "@types/chai": "^4.3.4",
    "@types/jest": "^29.5.13",
    "@types/shelljs": "^0.8.12",
    "chai": "^4.3.7",
    "jest": "^29.7.0",
    "protobufjs-cli": "^1.1.3",
    "shelljs": "^0.8.5",
    "ts-jest": "^29.2.5"
  },
  "engines": {
    "node": ">=20"
  }
}
